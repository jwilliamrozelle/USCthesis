	number = {6},
	urldate = {2021-09-30},
	journal = {Journal of Adolescent Health},
	author = {Beguy, Donatien and Kabiru, Caroline W. and Nderu, Evangeline N. and Ngware, Moses W.},
	month = dec,
	year = {2009},
	keywords = {Adolescents, Inconsistency, Informal settlements, Kenya, Nairobi, Sexual activity, Slum},
	pages = {595--601},
}


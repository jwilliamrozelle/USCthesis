@article{arriola_assessing_2016,
	title = {Assessing the {Risk} of {Hospital}-{Acquired} {Clostridium} {Difficile} {Infection} {With} {Proton} {Pump} {Inhibitor} {Use}: {A} {Meta}-{Analysis}},
	volume = {37},
	issn = {0899-823X, 1559-6834},
	shorttitle = {Assessing the {Risk} of {Hospital}-{Acquired} {Clostridium} {Difficile} {Infection} {With} {Proton} {Pump} {Inhibitor} {Use}},
	url = {https://www.cambridge.org/core/journals/infection-control-and-hospital-epidemiology/article/assessing-the-risk-of-hospitalacquired-clostridium-difficile-infection-with-proton-pump-inhibitor-use-a-metaanalysis/035B0AF5B1C524AC4DEC45281696C6EB},
	doi = {10.1017/ice.2016.194},
	abstract = {BACKGROUNDClostridium difficile is the principal infectious cause of antibiotic-associated diarrhea and accounts for 12\% of hospital-acquired infections. Recent literature has shown an increased risk of C. difficile infection (CDI) with proton pump inhibitor (PPI) use.OBJECTIVETo conduct a systematic assessment of the risk of hospital-acquired CDI following exposure to PPI.METHODSWe searched multiple databases for studies examining the relationship between PPI and hospital-acquired CDI. Pooled odds ratios were generated and assessment for heterogeneity performed.RESULTSWe found 23 observational studies involving 186,033 cases that met eligibility criteria. Across studies, 10,307 cases of hospital-acquired CDI were reported. Significant heterogeneity was present; therefore, a random effects model was used. The pooled odds ratio was 1.81 (95\% CI, 1.52–2.14), favoring higher risk of CDI with PPI use. Significant heterogeneity was present, likely due to differences in assessment of exposure, study population, and definition of CDI.DISCUSSIONThis meta-analysis suggests PPIs significantly increase the risk of hospital-acquired CDI. Given the significant health and economic burden of CDI and the risks of PPI, optimization of PPI use should be included in a multifaceted approach to CDI prevention.Infect Control Hosp Epidemiol 2016;1408–1417},
	language = {en},
	number = {12},
	urldate = {2023-04-09},
	journal = {Infection Control \& Hospital Epidemiology},
	author = {Arriola, Vanessa and Tischendorf, Jessica and Musuuza, Jackson and Barker, Anna and Rozelle, Jeffrey W. and Safdar, Nasia},
	month = dec,
	year = {2016},
	note = {Publisher: Cambridge University Press},
	pages = {1408--1417},
}

@article{deal_multidimensional_2014,
	title = {A multidimensional measurement of the health impact of community-based water treatment systems in {Uganda}},
	volume = {5},
	issn = {2043-9083},
	url = {https://doi.org/10.2166/washdev.2014.184},
	doi = {10.2166/washdev.2014.184},
	abstract = {Using a unique combination of ethnographic methods, healthcare facility chart reviews, and individual waterborne parasite tests, the health impacts of providing water treatment systems for communities in Uganda are compared to the impact measured using identical water technology and similar research methods in Honduras. While self-reported diarrhea rates improved in the Ugandan test communities when compared to controls, no significant impact was detected in any of the other measures. This contrasts sharply with findings in Honduras where all measures demonstrated statistically significant improvement after installation of identical water treatment systems. Ongoing ethnographic work reveals that knowledge of waterborne pathogens was universal in both Uganda and Honduras while practices related to water consumption varied greatly. Additional factors effecting these outcomes will be discussed.},

	urldate = {2023-01-30},
	journal = {PLOS ONE},
	author = {Ameyaw, Edward Kwabena and Budu, Eugene and Sambah, Francis and Baatiema, Linus and Appiah, Francis and Seidu, Abdul-Aziz and Ahinkorah, Bright Opoku},
	month = aug,
	year = {2019},
	note = {Publisher: Public Library of Science},
	keywords = {Angola, Contraceptives, Female contraception, Medical risk factors, Nigeria, Pregnancy, Regression analysis, Religion},

@article{kaljee_sexual_2007,
	series = {Special {Issue}: {Cultural} {Dynamics} in {HIV} {Prevention} {Among} {Young} {People}},
	title = {Sexual {Stigma}, {Sexual} {Behaviors}, and {Abstinence} {Among} {Vietnamese} {Adolescents}: {Implications} for {Risk} and {Protective} {Behaviors} for {HIV}, {Sexually} {Transmitted} {Infections}, and {Unwanted} {Pregnancy}},
